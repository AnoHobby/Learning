{
  "manifest_version": 3,
  "name": "GodField Script Replacer",
  "version": "1.0",
  "description": "GodFieldの main.dart.js をローカルのものに置き換える",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://godfield.net/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://godfield.net/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["main.dart.js"],
      "matches": ["https://godfield.net/*"]
    }
  ]
}
